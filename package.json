  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\""
  },
  "dependencies": {
    "bootstrap": "^4.6.0",
    "next": "10.0.7",
    "react": "17.0.1",
    "react-dom": "17.0.1",
